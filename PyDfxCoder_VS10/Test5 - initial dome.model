[DefaultFilter]
Type = DefaultFilter
InputFileList = "../Tests/Test5full.dxf"
Precision = 4
Normalize = Yes
Entities = NONE

	[[Origin]]
	X = 0
	Y = 0
	Z = 0

	[[Target]]
	X = Preset, Orthospheric, X #Mapping formula (preset or custom), Transformation type, Corresponding axis
	Y = Preset, Orthospheric, Y
	Z = Preset, Orthospheric, Z

	[[Transformation mapping]]
		[[[X]]]
		Mapping = X
		Origin = 0

		[[[Y]]]
		Mapping = Y
		Origin = 0

		[[[Z]]]
		Mapping = Z
		#Scale = 1
		Origin = 0

		[[[D]]]
		Scale = 1.0381

		[[[G]]]
		Scale = 1.0381

		[[[R]]]
		Scale = 1.0


#Filters for entities to be included in the model

[TestTendons]
Type = Filter
Layer = "working - tendons lower level", "working - tendons upper level"
Output = "DXF Output"
Entities = LINE
	[[Preprocess]]
	Data = '{"StiffnessMarker":"Tendon1"}'

[TestBases]
Type = Filter
Layer = "working - concrete bases"
Output = "DXF Output"
Entities = POLYLINE
PreprocessType = Preset
	[[Preprocess]]
	Function = "ExtrudeZ"
	Parameter = 0.35, 0.4, 0.35
	Data = '{"StiffnessMarker":"Conc1Dome"}', '{"StiffnessMarker":"Conc2Dome"}', '{"StiffnessMarker":"Conc3Dome"}'

#Output model and semantics
[Default Output]
Type = DefaultOutput
OutputFile = "../Tests - Results/Test5.dxf"
OutputType = Preset
Name = DXF

[DXF Output]
Type = Output
OutputFile = "../Tests - Results/Test5.dxf"
OutputType = Preset
Name = DXF

[Lira Output]
Type = Output
OutputFile = "../Tests - Results/Test5 spherical model.txt"
OutputType = Preset
Name = Lira-Custom
Format = Lira
Semantic = "Advanced Test"
	[[Actions]]
		[[[Properties]]]
		Type = ElementProperties
		Tendon1 = "S6 19.500E+006 15 0"
		Conc1Plate = "GEI 3.25E+006 0.3 0.4 RO 2.65"
		Conc2Plate = "GEI 3.25E+006 0.3 0.25 RO 2.65"
		Conc3Plate = "GEI 3.25E+006 0.3 0.25 RO 2.65"
		Conc4Plate = "GEI 3.25E+006 0.3 0.25 RO 2.65"
		Conc1Dome = "OBI 3.25E+006 0.3 RO 2.65"
		Conc2Dome = "OBI 3.25E+006 0.3 RO 2.65"
		Conc3Dome = "OBI 3.25E+006 0.3 RO 2.65"
		Conc4Dome = "OBI 3.25E+006 0.3 RO 2.65"
