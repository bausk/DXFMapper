[DefaultFilter]
Type = DefaultFilter
InputFileList = "../Tests/empty.dxf"
FilterOrder = "Boundaries3", "Boundaries1"
Precision = 2
Tolerance = 0.01
CheckNeighbors = Yes
IncludeInIndex = Yes
Normalize = Yes
Overkill = Yes
Entities = NONE
	[[Origin]]
	X = 0
	Y = 0
	Z = 0
	Delta = 0
	Gamma = 0
	R = 0

