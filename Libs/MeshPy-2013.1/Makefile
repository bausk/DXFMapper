.PHONY	: all install clean tags dist doc
# DO NOT EDIT THIS FILE -- it was generated by configure.py
# C:\Users\cadm\Documents\GitHub\PyDfxCoder\Libs\MeshPy-2013.1\configure.py

all: tags
	C:\Python27\python.exe setup.py build

dist:
	C:\Python27\python.exe setup.py sdist

install: tags
	C:\Python27\python.exe setup.py install

clean:
	rm -Rf build
	rm -f tags

tags:
	ctags -R src || true

doc:
	rm -Rf doc/html
	mkdir doc/html
	C:\Python27\python.exe `which epydoc` \
	  -o doc/html -v --html \
	  meshpy
